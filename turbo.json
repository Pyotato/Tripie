{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*", "!**/*.stories.{tsx,jsx,mdx}"],
      "outputs": [".next/**", "!.next/cache/**", "storybook-static/**", ".vercel/**"],
      "env": [
        "ALLOWED_METHODS",
        "ALLOWED_HEADERS",
        "MAX_AGE",
        "CREDENTIALS",
        "NEXT_PUBLIC_GOOGLE_MAP_API",
        "NEXT_PUBLIC_GOOGLE_PLACES_API",
        "NEXT_PUBLIC_GPT_API_KEY",
        "OPENAI_API_KEY",
        "NEXT_PUBLIC_CONTINENTL",
        "NEXT_PUBLIC_AWS_PLACE_ACTIONS",
        "NEXT_PUBLIC_AWS_MAP_ACTIONS",
        "NEXT_PUBLIC_AWS_ACCESS_KEY",
        "NEXT_PUBLIC_AWS_SECRET_KEY",
        "NEXT_PUBLIC_GITHUB_ID",
        "NEXT_PUBLIC_GITHUB_SECRET",
        "NEXTAUTH_URL",
        "KAKAO_CLIENT_ID",
        "KAKAO_CLIENT_SECRET",
        "NEXT_PUBLIC_GITHUB_ID",
        "NEXT_PUBLIC_GITHUB_SECRET",
        "NEXTAUTH_SECRET",
        "NX_DAEMON",
        "POSTGRES_DATABASE",
        "POSTGRES_HOST",
        "POSTGRES_PASSWORD",
        "POSTGRES_PRISMA_URL",
        "connect_timeout",
        "POSTGRES_URL",
        "POSTGRES_URL_NON_POOLING",
        "POSTGRES_USER",
        "TURBO_REMOTE_ONLY",
        "TURBO_RUN_SUMMARY",
        "VERCEL",
        "VERCEL_ENV",
        "VERCEL_URL"
      ]
    },
    "build:storybook": {
      "env": ["CHROMATIC_TOKEN"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:deploy": {
      "cache": false
    },
    "db:migrate": {
      "cache": false,
      "persistent": true
    },
    "typecheck": {}
  },
  "globalDependencies": [".env"]
}
